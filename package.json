{
  "name": "node-task3",
  "version": "1.0.0",
  "description": "air bnb es6 standard and prettier eslint installation",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon index",
    "lint": "eslint ./ --ext .js  --ignore-pattern node_modules",
    "lint:fix": "eslint ./ --fix=true",
    "format": "prettier -w .",
    "prepare": "husky install",
    "docs": "apidoc -i ./src/routes -o public/apidoc/",
    "deploy": "gh-pages -d public/apidoc -r https://github.com/mariyamh/carbooking.git"
  },
  "keywords": [
    "airbnb",
    "eslint",
    "prettier",
    "es6",
    "standards"
  ],
  "author": "maryam saleem",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "gh-pages": "^3.2.1",
    "husky": "^6.0.0",
    "lint-staged": "^7.3.0",
    "prettier": "^2.3.1"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "-": "0.0.1",
    "apidoc": "^0.28.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "g": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.9",
    "mongoose": "^5.12.13",
    "mongose": "0.0.2-security",
    "nodemon": "^2.0.7",
    "sequelize": "^6.6.2"
  }
}
