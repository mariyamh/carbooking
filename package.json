{
  "name": "node-task3",
  "version": "1.0.0",
  "description": "air bnb es6 standard and prettier eslint installation",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index",
    "lint": "eslint src/ --ext=js,jsx,ts,tsx --ignore-pattern node_modules",
    "lint:fix": "eslint src/ --fix=true --ext=js,jsx,ts,tsx --ignore-pattern node_modules",
    "format": "prettier -w .",
    "prepare": "husky install"
  },
  "keywords": [
    "airbnb",
    "eslint",
    "prettier",
    "es6",
    "standards"
  ],
  "author": "maryam saleem",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.1"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
